def print_calender(days, start_day):
    print("M  T  W  T  F  S  S")
    print("  " * start_day, end="")

    for day in range(1, days + 1):
        print(f"{day:2}", end=" ")
        start_day += 1
        if start_day == 7:
            print()
            start_day = 0

    print()

days = int(input("Enter the number of days in the month: "))
start_day = int(input("Enter the starting day (0 for Monday, 1 for Tuesday, ..., 6 for sunday):"))

print_calender(days, start_day)
